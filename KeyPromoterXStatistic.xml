<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="New Folder...">
        <Item>
          <IdeaActionID>FileChooser.NewFolder</IdeaActionID>
          <ShortCut>'Alt+Insert' or 'Ctrl+N'</ShortCut>
          <Description>New Folder...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'example-mcmod build'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'example-mcmod build'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>